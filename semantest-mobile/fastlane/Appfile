# Fastlane Appfile for Semantest Mobile

# iOS
app_identifier("com.semantest.app")
apple_id(ENV["APPLE_ID"])
itc_team_id(ENV["ITC_TEAM_ID"])
team_id(ENV["TEAM_ID"])

# Android
json_key_file(ENV["PLAY_CONSOLE_JSON_KEY"])
package_name("com.semantest.app")

